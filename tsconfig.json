{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "node",
    "listEmittedFiles": true,
    "skipLibCheck": true,
    "declaration": true,
  },
  "exclude": [
    "types",
    "docs",
    "helpers",
  ],
  "forceConsistentCasingInFileNames": true,
  "esModuleInterop": true,
  "allowJs": true
}
